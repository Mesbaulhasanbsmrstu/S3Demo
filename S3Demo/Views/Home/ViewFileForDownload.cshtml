@model S3FileDetails

<table>
    <tr>
        <th>FileName</th>
    </tr>
    <tr>
        <td> @Html.DisplayFor(m => m.FileName)</td>
    </tr>
</table>
<form asp-action="DownloadFile">
    <input type="hidden" asp-for="FileName" />
    <input type="submit" value="Download" class="btn btn-danger" />
</form>
